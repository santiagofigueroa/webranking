@model InfoTrack.WebRanking.Models.SearchResult

@using (Html.BeginForm("Search", "Search", FormMethod.Post))
{
    <div class="form-group">
        @Html.LabelFor(m => m.SelectedSearchEngineName, "Search Engine")
        @Html.DropDownListFor(m => m.SelectedSearchEngineName, Model.AvailableSearchEngines, new { @class = "form-control" })
    </div>

    <div class="form-group">
        @Html.LabelFor(m => m.Keywords)
        @Html.TextBoxFor(m => m.Keywords, new { @class = "form-control" })
    </div>

    <input type="submit" value="Search" class="btn btn-primary" />
}
